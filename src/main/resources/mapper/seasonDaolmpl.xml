<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.angzhao.dao.seasonDao">
    <insert id="insertSeasonId" parameterType="com.angzhao.entity.foodEntity">
        INSERT INTO season_info(food_id)
        VALUES (#{foodId})
    </insert>

    <delete id="deleteSeasonId" parameterType="com.angzhao.entity.foodEntity">
        DELETE FROM season_info
        WHERE food_id = #{foodId}
    </delete>

    <select id="querySeasonId" resultType="com.angzhao.entity.foodEntity">
        SELECT * FROM season_info
        WHERE food_id = #{foodId}
    </select>
</mapper>